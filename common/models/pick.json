{
  "name": "Pick",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true,
    "postgresql": {
      "scheme": "picksapp",
      "table": "Pick"
    }
  },
  "properties": {
    "selectedWinner": {
      "type": "string",
      "required": true
    },
    "participant": {
      "type": "number",
      "required": true
    },
    "selectedPoints": {
      "type": "number",
      "required": true,
      "default": 1
    },
    "week": {
      "type": "number",
      "required": true
    }
  },
  "validations": [],
  "relations": {},
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$authenticated",
      "permission": "ALLOW"
    }
  ],
  "methods": {}
}
